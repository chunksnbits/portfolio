{
  "name": "portfolio",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "vuepress dev docs",
    "build": "vuepress build docs",
    "lint": "echo '/** noop */'"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "commitizen": "^3.0.7",
    "commitlint": "^8.0.0",
    "husky": "^2.4.1",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "vuepress": "^1.0.1"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "header-max-length": [
        0,
        "always",
        100
      ]
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.js"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "yarn commitlint -e $GIT_PARAMS",
      "pre-push": "yarn lint"
    }
  },
  "dependencies": {
    "markdown-it-link-attributes": "^2.1.0",
    "vue-svg-inline-loader": "^1.2.15",
    "vue-svg-loader": "^0.12.0",
    "vuepress-plugin-container": "^2.0.1",
    "vuetify": "^1.5.14"
  }
}
